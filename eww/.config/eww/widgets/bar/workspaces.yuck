(deflisten swayworkspaces
	:initial "${[]}"
	"ewwswayworkspaces"
)
(defwidget workspaces []
	(box
		:space-evenly false
		:class "outputs"
		(for output in "${jq(swayworkspaces, "keys | reverse")}"
			(box
				:class "workspaces"
				(for workspace in { swayworkspaces[output] }
					(label
						:class "workspace ${workspace.focused ? "focused" : ""}"
						:text "${workspace.name}"
					)
				)
			)
		)
	)
)
